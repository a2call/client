/* @flow */

import React, {Component} from 'react'
import type {IconsType, FontIconsType} from './icon.constants'
export type IconType = IconsType | FontIconsType

export type Props = {
  type: IconType,
  hint?: string,
  onClick?: () => void,
  onMouseEnter?: () => void,
  onMouseLeave?: () => void,
  style?: Object,
  inheritColor?: boolean,
  underlayColor?: string,
  className?: string
}

declare export default class Icon extends React.Component {
  props: Props;
}
